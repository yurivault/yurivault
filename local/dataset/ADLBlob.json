{
	"name": "ADLBlob",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeExecAgroline",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Directory": {
				"type": "string",
				"defaultValue": "Inbox"
			},
			"Container": {
				"type": "string",
				"defaultValue": "storagemaster"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"folderPath": {
					"value": "@dataset().Directory",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().Container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ";",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": [
			{
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}